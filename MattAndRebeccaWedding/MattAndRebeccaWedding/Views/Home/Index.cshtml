@{
    ViewBag.Title = "Matt and Rebecca Home";
}

@Html.Partial("_countdown")   

<div class="well addGuestContent starsHeaderWell">
    <div class="row">
        <div class="col-xs-12">

            <img src="~/Content/Images/starsHeader.png" class="img-responsive center-block" />
        </div>
    </div>
    <div class="row" style="padding-bottom:30px;">
        <div class="col-xs-12">
            <img src="~/Content/Images/nameBannerWithLine.png" class="img-responsive center-block" />
        </div>
    </div>
    <div class="row" style="padding-bottom:50px;">
        <div class="col-xs-12 col-sm-6" style="padding-bottom:30px;">
            <img src="~/Content/Images/tyingTheKnot.png" class="img-responsive center-block" />
        </div>
        <div class="col-xs-12 col-sm-6">
            <img src="~/Content/Images/fakePhoto.jpg" class="img-responsive imageShadow" />
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <img src="~/Content/Images/Welcome.png" class="img-responsive center-block" />
        </div>
        <div class="col-xs-offset-2 col-xs-8 text-center">
            <h3>
                Thanks for visiting. We are so glad you are here and look forward to seeing you in the fall for our wedding!
            </h3>
        </div>
    </div>
    <div class="row" style="padding-bottom:50px;">
        <div class="col-xs-12 col-md-6 col-md-offset-3">
            <img src="~/Content/Images/nameCircle.png" class="img-responsive center-block" />
        </div>
    </div>
</div>

